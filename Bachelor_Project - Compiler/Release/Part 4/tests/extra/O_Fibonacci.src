var x:int, y:int;

func fibonacci(n:int):int
var x:int;
{
    if n == 0 then
        x = 1;
    else if n == 1 then
        x = 1;
    else 
        x = fibonacci(n - 1) + fibonacci(n - 2);

    # write x;
    return x;
}
end fibonacci

x = 2;

while x < 6 do
{
    # write 0;
    y = fibonacci(x);
    write y;
    x = x + 1;
}

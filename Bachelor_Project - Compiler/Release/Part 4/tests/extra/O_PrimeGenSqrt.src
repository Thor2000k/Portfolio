func IsPrime(x:int):bool
var m:int, s:int;
{
    m = 3;
    if x == 2 then return true;
    else if x % 2 == 0 then return false;

    s = sqrt(x) + 1;
    while (m < s && !(x % m == 0)) do m = m + 2;
    return m >= s;
}
end IsPrime

var n:int, i:int, count:int;

n = 100000;
i = 2;

while i <= n do
{
    if IsPrime(i) then
        count = count + 1;

    i = i + 1;
}

write count;
